    width: 30px;
    height: 30px;
    margin-right: 5px;
}

.b2b-alert-list div span {
    width: 20px;
    height: 20px;
    font-size: .8rem;
    padding: 10px 6px;
    display: block;
    position: absolute;
    top: 8px;
    right: 8px;
    background: rgba(0, 0, 0, 0.8);
    border-radius: 50%;
