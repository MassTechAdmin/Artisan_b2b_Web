/* Trip List */

.b2b-trip-list {
    width: 80%;
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
    margin-top: 2%;
}

.trip-hand {
    display: flex;
    flex-wrap: wrap;
    width: 100%;
    justify-content: space-between;
}

.gaint-select {
    position: relative;
    width: 49%;
    height: 90px;
    box-sizing: border-box;
    background: #8c001a;
    background: linear-gradient(#8c001a, #a21d1d, #8c001a);
}

.gaint-select a {
    display: block;
    padding: 30px;
}

.luxe-select {
    position: relative;
    width: 49%;
    height: 90px;
    box-sizing: border-box;
    background: linear-gradient(#4b1b88, #6309d6, #4b1b88);
}

.luxe-select a {
    display: block;
    padding: 30px;
}

.gaint-select::after,
.luxe-select::after {
    content: '更多行程▼';
    position: absolute;
    right: 10px;
    color: #fff;
    font-size: 1rem;
    pointer-events: none;
    top: -50%;
    transform: translateY(-50%);
    transition: .5s;
    overflow: hidden;
}

.gaint-select:hover::after,
.luxe-select:hover::after {
    top: 50%;
}

.trip-hand h2 {
    color: #fff;
    font-weight: 500;
}

.trip-wrap-tool {
    width: 100%;
    overflow: hidden;
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
    align-items: flex-start;
}

.trip-wrap {
    width: 100%;
    display: flex;
    justify-content: space-between;
    flex-wrap: wrap;
}

.trip-box {
    width: 49%;
    position: relative;
    margin-top: 1.7%;
    border: 1px solid #cccccc;
    border-radius: 4px 4px 0 0;
}

.trip-box-more {
    position: absolute;
    background: #67b72d;
    border-radius: 3px;
    color: #fff;
    padding: 4px 10px;
    box-sizing: border-box;
    top: 7px;
    right: 3%;
    transition: .3s;
    font-size: .8rem;
}

.trip-box-more:hover {
    background: #4a9a0f;
}

.trip-box-tittle {
    background: linear-gradient(#fff, #fbfbfb, #e5e5e5);
    border-radius: 5px 5px 0 0;
    height: 40px;
    margin-bottom: 2%;
}

.trip-box-tittle h3 {
    text-align: center;
    color: #67b72d;
    font-size: 1.2rem;
    padding: .4rem 0;
    box-sizing: border-box;
    font-weight: 500;
}

.trip-box ul li {
    padding: 2px 10px;
    margin: 8px 2%;
    box-sizing: border-box;
    border-bottom: 1px dotted #a0a0a0;
}

.trip-box ul li:last-of-type {
    border-bottom: none;
}

.trip-box ul li a {
    display: flex;
    justify-content: space-between;
    transition: .5s;
    display: block;
}

.ellipsis {
    width: calc(100% - 115px);
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    font-size: .9375rem;
    color: #222;
    font-weight: 700;
    display: inline-block;
}

.ellipsis span {
    color: #ff743c;
}

.trip-box ul li a:hover .ellipsis {
    color: #6d6d6d;
}

.trip-box ul li .price {
    color: #f55520;
    text-align: right;
    width: 115px;
    display: inline-block;
    vertical-align: top;
}

.trip-box ul li .price::before {
    content: 'NET.';
    font-size: .85rem;
    margin-right: 2px;
}

.trip-box ul li .price::after {
    content: '起';
    font-size: .85rem;
    margin-left: 2px;
}

@media screen and (max-width:1023px) {
    .b2b-trip-list {
        width: 100%;
    }
    .gaint-select::after,
    .luxe-select::after {
        top: 50%;
    }
    .trip-wrap {
        width: 100%;
    }
    .trip-box {
        padding-bottom: 50px;
    }
    .trip-box-more {
        right: 50%;
        transform: translateX(50%);
        bottom: 10px;
        top: auto;
        padding: 5px 15px;
        font-size: 1rem;
    }
    .lecture_m img,
    .season_sale_m img,
    .dm_download_m img {
        width: 100%;
        height: auto;
    }
}

@media screen and (max-width:767px) {
    .gaint-select {
        width: 100%;
    }
    .luxe-select {
        margin-top: 2%;
        margin-bottom: 3%;
        width: 100%;
    }
    .trip-box {
        width: 100%;
        margin-top: 5%;
    }
}

@media screen and (max-width:500px) {
    .trip-box ul li a {
        flex-wrap: wrap;
        justify-content: flex-end;
    }
    .trip-box ul li .price {
        margin-top: 10px;
    }
}


